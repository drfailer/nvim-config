{
  "HEAD": {
    "prefix": ["head"],
    "body": [
      "---",
      "title: $0",
      "date: \"`!v strftime(\"%Y-%m-%d\")`\"",
      "author: \"$1\"",
      "tags: [$2]",
      "---"
    ],
    "description": "head info"
  },
  "LINK": {
    "prefix": ["link"],
    "body": [
      "[${1:desc}](${2:link})$0"
    ],
    "description": "link"
  },
  "IMG": {
    "prefix": ["img"],
    "body": [
      "![${1:desc}](${2:path})$0"
    ],
    "description": "images"
  }

}
