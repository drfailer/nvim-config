{
  "HEAD": {
    "prefix": ["head"],
    "body": [
      "---",
      "title: $1",
      "date: ${VIM:system('date +%d-%m-%Y')}",
      "author: \"${VIM:\\$USER}\"",
      "tags: [$2]",
      "margin: 2cm",
      "---",
      "$0"
    ],
    "description": "head info"
  },
  "LINK": {
    "prefix": ["link"],
    "body": [
      "[${1:desc}](${2:link})$0"
    ],
    "description": "link"
  },
  "IMG": {
    "prefix": ["img"],
    "body": [
      "![${1:desc}](${2:path})$0"
    ],
    "description": "images"
  }

}
