##### emphasis
# bold
snippet b
	\textbf{${1}}${0}
snippet tt
	\textit{${1}}${0}
##### section
snippet sec
	\section{${1}}${0}
snippet subsec
	\subsection{${1}}${0}
##### parts
snippet begin
	\begin{${1:document}}
		${0}
	\end{${1:document}}
##### listes
# item
snippet item
	\begin{item}
		${0}
	\end{item}
# it
snippet it
	\item ${0}
